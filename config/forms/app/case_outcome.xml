      <instance>
        <case_outcome id="case_outcome" prefix="J1!case_outcome!" delimiter="#" version="1">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
              <role/>
              <phone/>
            </user>
            <contact>
              <_id/>
              <name/>
              <patient_id/>
              <parent>
                <_id/>
                <name/>
                <parent>
                  <_id/>
                  <name/>
                  <parent>
                    <_id/>
                    <name/>
                    <parent>
                      <_id/>
                      <name/>
                    </parent>
                  </parent>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_uuid tag="hidden"/>
          <prerequisites tag="hidden">
            <cases_group>
              <_id/>
              <name/>
              <outbreak_location>
                <_id/>
                <name/>
                <ol_county/>
                <ol_subcounty/>
                <outbreak>
                  <_id/>
                  <name/>
                  <code/>
                  <disease/>
                  <disease_name/>
                  <date/>
                  <status/>
                </outbreak>
              </outbreak_location>
            </cases_group>
            <case_uuid/>
            <case_id/>
            <case_name/>
            <outbreak_code/>
            <outbreak_name/>
            <outbreak_disease/>
            <outbreak_disease_name/>
            <outbreak_date/>
            <outbreak_status/>
            <outbreak_location_name/>
            <outbreak_location_county/>
            <outbreak_location_subcounty/>
            <case_outbreak_title/>
            <case_disease_title/>
            <case_outbreak_location_title/>
            <case_name_title/>
            <case_id_title/>
            <case_outcome_title/>
          </prerequisites>
          <co_laboratory>
            <col_sample_collected>no</col_sample_collected>
            <col_sample_information>
              <col_case_id/>
              <col_sample_date/>
              <col_sample_type/>
              <col_sample_type_other/>
              <col_rdt/>
              <col_rdt_group>
                <col_rdt_results>pending</col_rdt_results>
                <col_rdt_results_date/>
              </col_rdt_group>
              <col_lab/>
              <col_lab_group>
                <col_lab_results>pending</col_lab_results>
                <col_lab_results_date/>
              </col_lab_group>
            </col_sample_information>
          </co_laboratory>
          <co_final>
            <cof_case_classification/>
            <cof_patient_status/>
            <cof_referral_reason/>
            <cof_date/>
          </co_final>
          <co_created>
            <created_by/>
            <created_on/>
          </co_created>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </case_outcome>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/case_outcome/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/case_outcome/inputs/source" type="string"/>
      <bind nodeset="/case_outcome/inputs/user/contact_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/user/facility_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/user/name" type="string"/>
      <bind nodeset="/case_outcome/inputs/user/role" type="string"/>
      <bind nodeset="/case_outcome/inputs/user/phone" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/name" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/name" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/name" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/parent/name" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/case_outcome/inputs/contact/parent/parent/parent/parent/name" type="string"/>
      <bind nodeset="/case_outcome/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/_id" type="string" readonly="true()" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/name" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/_id" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/name" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/ol_county" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/ol_subcounty" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/_id" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/name" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/code" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/disease" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/disease_name" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/date" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak/status" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/case_uuid" type="string" calculate="../../inputs/contact/_id"/>
      <bind nodeset="/case_outcome/prerequisites/case_id" type="string" calculate="../../inputs/contact/patient_id"/>
      <bind nodeset="/case_outcome/prerequisites/case_name" type="string" calculate="../../inputs/contact/name"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_code" type="string" calculate="../cases_group/outbreak_location/outbreak/code"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_name" type="string" calculate="../cases_group/outbreak_location/outbreak/name"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_disease" type="string" calculate="../cases_group/outbreak_location/outbreak/disease"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_disease_name" type="string" calculate="../cases_group/outbreak_location/outbreak/disease_name"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_date" type="string" calculate="../cases_group/outbreak_location/outbreak/date"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_status" type="string" calculate="../cases_group/outbreak_location/outbreak/status"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_location_name" type="string" calculate="../cases_group/outbreak_location/name"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_location_county" type="string" calculate="../cases_group/outbreak_location/ol_county"/>
      <bind nodeset="/case_outcome/prerequisites/outbreak_location_subcounty" type="string" calculate="../cases_group/outbreak_location/ol_subcounty"/>
      <bind nodeset="/case_outcome/prerequisites/case_outbreak_title" readonly="true()" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/case_disease_title" readonly="true()" type="string" relevant=" /case_outcome/prerequisites/cases_group/outbreak_location/outbreak/disease_name  != ''"/>
      <bind nodeset="/case_outcome/prerequisites/case_outbreak_location_title" readonly="true()" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/case_name_title" readonly="true()" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/case_id_title" readonly="true()" type="string"/>
      <bind nodeset="/case_outcome/prerequisites/case_outcome_title" readonly="true()" type="string"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_collected" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information" relevant="selected( /case_outcome/co_laboratory/col_sample_collected , 'yes')"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_case_id" type="string" readonly="true()" required="true()" calculate=" /case_outcome/prerequisites/case_uuid "/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_sample_date" type="date" required="true()" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_sample_type" type="select" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_sample_type_other" type="string" required="true()" relevant="selected( /case_outcome/co_laboratory/col_sample_information/col_sample_type , 'other')"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_rdt" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_rdt_group" relevant="selected( /case_outcome/co_laboratory/col_sample_information/col_rdt , 'yes')"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_rdt_group/col_rdt_results" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_rdt_group/col_rdt_results_date" type="date" required="true()" relevant=" /case_outcome/co_laboratory/col_sample_information/col_rdt_group/col_rdt_results  != 'pending'" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_lab" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_lab_group" relevant="selected( /case_outcome/co_laboratory/col_sample_information/col_lab , 'yes')"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_lab_group/col_lab_results" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_laboratory/col_sample_information/col_lab_group/col_lab_results_date" type="date" required="true()" relevant=" /case_outcome/co_laboratory/col_sample_information/col_lab_group/col_lab_results  != 'pending'" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))"/>
      <bind nodeset="/case_outcome/co_final/cof_case_classification" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_final/cof_patient_status" type="select1" required="true()"/>
      <bind nodeset="/case_outcome/co_final/cof_referral_reason" type="string" required="true()" relevant="selected( /case_outcome/co_final/cof_patient_status , 'referred')"/>
      <bind nodeset="/case_outcome/co_final/cof_date" type="date" required="true()" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))"/>
      <bind nodeset="/case_outcome/co_created/created_by" type="string" readonly="true()" calculate="../../inputs/user/name"/>
      <bind nodeset="/case_outcome/co_created/created_on" type="date" readonly="true()" calculate="today()"/>
      <bind nodeset="/case_outcome/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/case_outcome/inputs">
      <group ref="/case_outcome/inputs/user"/>
      <group appearance="field-list" ref="/case_outcome/inputs/contact">
        <group ref="/case_outcome/inputs/contact/parent">
          <group ref="/case_outcome/inputs/contact/parent/parent">
            <group ref="/case_outcome/inputs/contact/parent/parent/parent">
              <group ref="/case_outcome/inputs/contact/parent/parent/parent/parent"/>
            </group>
          </group>
        </group>
      </group>
    </group>
    <group appearance="field-list" ref="/case_outcome/prerequisites">
      <group ref="/case_outcome/prerequisites/cases_group">
        <input appearance="hidden select-contact type-cases_group" ref="/case_outcome/prerequisites/cases_group/_id">
          <label ref="jr:itext('/case_outcome/prerequisites/cases_group/_id:label')"/>
        </input>
        <group ref="/case_outcome/prerequisites/cases_group/outbreak_location">
          <group ref="/case_outcome/prerequisites/cases_group/outbreak_location/outbreak"/>
        </group>
