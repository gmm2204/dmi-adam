<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>User</h:title>
    <model>
      <itext>
        <translation lang="en">
          <text id="/data/init/parent_id:hint">
            <value><output value=" /data/init/type_label "/></value>
          </text>
          <text id="/data/init/parent_id:label">
            <value>At</value>
          </text>
          <text id="/data/init/type_selector/district_hospital:label">
            <value>District Hospital</value>
          </text>
          <text id="/data/init/type_selector/outbreak:label">
            <value>Outbreak</value>
          </text>
          <text id="/data/inputs/user/contact_id:label">
            <value>Contact ID of the logged in user</value>
          </text>
          <text id="/data/inputs/user/facility_id:label">
            <value>Place ID of the logged in user</value>
          </text>
          <text id="/data/inputs/user/name:label">
            <value>Name of the logged in user</value>
          </text>
          <text id="/data/person/email:label">
            <value>Email Address</value>
          </text>
          <text id="/data/person/first_name:label">
            <value>First name</value>
          </text>
          <text id="/data/person/last_name:label">
            <value>Middle name</value>
          </text>
          <text id="/data/person/parent:label">
            <value>Parent ID</value>
          </text>
          <text id="/data/person/phone:label">
            <value>Phone Number</value>
          </text>
          <text id="/data/person/role/CHP:label">
            <value>CHP</value>
          </text>
          <text id="/data/person/role/CHP_Supervisor:label">
            <value>CHP Supervisor</value>
          </text>
          <text id="/data/person/role:hint">
            <value>Select the role that best suits this person</value>
          </text>
          <text id="/data/person/role:label">
            <value>Role</value>
          </text>
          <text id="/data/person/surname:label">
            <value>Last name</value>
          </text>
          <text id="/data/person/type:label">
            <value>Person type</value>
          </text>
        </translation>
        <translation lang="sw">
          <text id="/data/init/parent_id:hint">
            <value><output value=" /data/init/type_label "/></value>
          </text>
          <text id="/data/init/parent_id:label">
            <value>Ni wa</value>
          </text>
          <text id="/data/init/type_selector/district_hospital:label">
            <value>-</value>
          </text>
          <text id="/data/init/type_selector/outbreak:label">
            <value>-</value>
          </text>
          <text id="/data/inputs/user/contact_id:label">
            <value>Namba ya Usajilisho ya mtumiaji wa huduma hii</value>
          </text>
          <text id="/data/inputs/user/facility_id:label">
            <value>Kitambulisho cha mahali cha mtumizi wa huduma hii</value>
          </text>
          <text id="/data/inputs/user/name:label">
            <value>Jina ya mtumizi</value>
          </text>
          <text id="/data/person/email:label">
            <value>-</value>
          </text>
          <text id="/data/person/first_name:label">
            <value>-</value>
          </text>
          <text id="/data/person/last_name:label">
            <value>-</value>
          </text>
          <text id="/data/person/parent:label">
            <value>ID ya mzazi</value>
          </text>
          <text id="/data/person/phone:label">
            <value>-</value>
          </text>
          <text id="/data/person/role/CHP:label">
            <value>-</value>
          </text>
          <text id="/data/person/role/CHP_Supervisor:label">
            <value>-</value>
          </text>
          <text id="/data/person/role:hint">
            <value>Chagua jukumu linachoambatana na huyu mtu</value>
          </text>
          <text id="/data/person/role:label">
            <value>Jukumu</value>
          </text>
          <text id="/data/person/surname:label">
            <value>-</value>
          </text>
          <text id="/data/person/type:label">
            <value>Jina</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="contact:person:create" prefix="J1!contact:person:create!" delimiter="#" version="2023-11-12 22-44">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <person>
            <parent>PARENT</parent>
            <type>person</type>
            <role>patient</role>
            <first_name/>
            <last_name/>
            <surname/>
            <phone/>
            <email/>
            <name/>
            <role_label/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </person>
          <init>
            <parent_id/>
            <name/>
            <type/>
            <type_selector/>
            <type_label/>
          </init>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/parent_id" type="string" readonly="true()" calculate=" /data/person/parent "/>
      <bind nodeset="/data/init/name" type="string"/>
      <bind nodeset="/data/init/type" type="string"/>
      <bind nodeset="/data/init/type_selector" type="select1" calculate="../type"/>
      <bind nodeset="/data/init/type_label" type="string" calculate="jr:choice-name( /data/init/type_selector ,' /data/init/type_selector ')"/>
      <bind nodeset="/data/person/parent" type="string"/>
      <bind nodeset="/data/person/type" type="string"/>
      <bind nodeset="/data/person/role" type="select1" required="true()"/>
      <bind nodeset="/data/person/first_name" type="string" required="true()"/>
      <bind nodeset="/data/person/last_name" type="string"/>
      <bind nodeset="/data/person/surname" type="string" required="true()"/>
      <bind nodeset="/data/person/phone" type="tel" required="true()"/>
      <bind nodeset="/data/person/email" type="string" required="true()"/>
      <bind nodeset="/data/person/name" type="string" calculate="concat( /data/person/first_name , ' ',  /data/person/last_name , ' ',  /data/person/surname )"/>
      <bind nodeset="/data/person/role_label" type="string" calculate="jr:choice-name( /data/person/role ,' /data/person/role ')"/>
      <bind nodeset="/data/person/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/person/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/person/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id">
          <label ref="jr:itext('/data/inputs/user/contact_id:label')"/>
        </input>
        <input ref="/data/inputs/user/facility_id">
          <label ref="jr:itext('/data/inputs/user/facility_id:label')"/>
        </input>
        <input ref="/data/inputs/user/name">
          <label ref="jr:itext('/data/inputs/user/name:label')"/>
        </input>
      </group>
    </group>
    <group appearance="hidden" ref="/data/init">
      <input appearance="db-object" ref="/data/init/parent_id">
        <label ref="jr:itext('/data/init/parent_id:label')"/>
        <hint ref="jr:itext('/data/init/parent_id:hint')"/>
      </input>
      <input appearance="hidden" ref="/data/init/name"/>
      <input appearance="hidden" ref="/data/init/type"/>
      <select1 appearance="hidden" ref="/data/init/type_selector">
        <item>
          <label ref="jr:itext('/data/init/type_selector/district_hospital:label')"/>
          <value>district_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/type_selector/outbreak:label')"/>
          <value>outbreak</value>
        </item>
      </select1>
    </group>
    <group ref="/data/person">
      <select1 appearance="minimal" ref="/data/person/role">
        <label ref="jr:itext('/data/person/role:label')"/>
        <hint ref="jr:itext('/data/person/role:hint')"/>
        <item>
          <label ref="jr:itext('/data/person/role/CHP:label')"/>
          <value>CHP</value>
        </item>
        <item>
          <label ref="jr:itext('/data/person/role/CHP_Supervisor:label')"/>
          <value>CHP_Supervisor</value>
        </item>
      </select1>
      <input ref="/data/person/first_name">
        <label ref="jr:itext('/data/person/first_name:label')"/>
      </input>
      <input ref="/data/person/last_name">
        <label ref="jr:itext('/data/person/last_name:label')"/>
      </input>
      <input ref="/data/person/surname">
        <label ref="jr:itext('/data/person/surname:label')"/>
      </input>
      <input ref="/data/person/phone">
        <label ref="jr:itext('/data/person/phone:label')"/>
      </input>
      <input ref="/data/person/email">
        <label ref="jr:itext('/data/person/email:label')"/>
      </input>
      <group appearance="hidden" ref="/data/person/meta"/>
    </group>
  </h:body>
</h:html>
