<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms">
  <h:head>
    <h:title>New Outbreak</h:title>
    <model>
      <itext>
        <translation lang="en">
          <text id="/data/cases_group/cha_outbreak_location_title:label">
            <value>&lt;h4 style=&quot;font-weight: normal; border-bottom: rgb(150, 150, 150) dashed 1px; padding: 15px 0&quot;&gt;Outbreak Location: <output value=" /data/cases_group/outbreak_location_name "/>&lt;/h4&gt;</value>
          </text>
          <text id="/data/cases_group/cha_outbreak_title:label">
            <value>&lt;h4 style=&quot;font-weight: normal; border-bottom: rgb(150, 150, 150) dashed 1px; padding: 15px 0&quot;&gt;Outbreak: <output value=" /data/cases_group/outbreak_name "/>&lt;/h4&gt;</value>
          </text>
          <text id="/data/cases_group/name:label">
            <value>Cases Group Name</value>
          </text>
          <text id="/data/cases_group/outbreak_location/_id:label">
            <value>Outbreak Location</value>
          </text>
          <text id="/data/cases_group/outbreak_location/name:label">
            <value>Name</value>
          </text>
          <text id="/data/cases_group/outbreak_location/outbreak/code:label">
            <value>Code</value>
          </text>
          <text id="/data/cases_group/outbreak_location/outbreak/disease:label">
            <value>Disease</value>
          </text>
          <text id="/data/cases_group/outbreak_location/outbreak/disease_name:label">
            <value>Disease name</value>
          </text>
          <text id="/data/cases_group/outbreak_location/outbreak/name:label">
            <value>Name</value>
          </text>
          <text id="/data/contact/title:label">
            <value>A primary contact associated with this &lt;strong&gt;cases group&lt;/strong&gt; will be automatically created.</value>
          </text>
          <text id="/data/init/create_new_person:label">
            <value>Set the Primary Contact</value>
          </text>
          <text id="/data/init/select_person:label">
            <value>Select the Primary Contact</value>
          </text>
        </translation>
      </itext>
      <instance>
        <data id="contact:cases_group:create" prefix="J1!contact:cases_group:create!" delimiter="#" version="2024-01-22 11-08">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <cases_group>
            <parent>PARENT</parent>
            <type>cases_group</type>
            <outbreak_name/>
            <outbreak_code/>
            <outbreak_location_name/>
            <cha_outbreak_title/>
            <cha_outbreak_location_title/>
            <outbreak_location>
              <_id/>
              <name/>
              <outbreak>
                <name/>
                <code/>
                <disease/>
                <disease_name/>
              </outbreak>
            </outbreak_location>
            <name/>
            <contact/>
            <metas>
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </metas>
          </cases_group>
          <init>
            <create_new_person>new_person</create_new_person>
            <select_person/>
          </init>
          <contact>
            <parent>PARENT</parent>
            <type>person</type>
            <name/>
            <title/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </contact>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/create_new_person" type="select1" required="true()" readonly="true()"/>
      <bind nodeset="/data/init/select_person" type="db:person" relevant="selected( /data/init/create_new_person ,'old_person')" required="true()"/>
      <bind nodeset="/data/contact/parent" type="string"/>
      <bind nodeset="/data/contact/type" type="string"/>
      <bind nodeset="/data/contact/name" type="string" calculate="concat(../../cases_group/name, &quot; - Primary Contact&quot;)" readonly="true()"/>
      <bind nodeset="/data/contact/title" readonly="true()" type="string"/>
      <bind nodeset="/data/contact/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/contact/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/contact/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/cases_group/parent" type="string"/>
      <bind nodeset="/data/cases_group/type" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_name" type="string" calculate="../../cases_group/outbreak_location/outbreak/name"/>
      <bind nodeset="/data/cases_group/outbreak_code" type="string" calculate="../../cases_group/outbreak_location/outbreak/code"/>
      <bind nodeset="/data/cases_group/outbreak_location_name" type="string" calculate="../../cases_group/outbreak_location/name"/>
      <bind nodeset="/data/cases_group/cha_outbreak_title" readonly="true()" type="string"/>
      <bind nodeset="/data/cases_group/cha_outbreak_location_title" readonly="true()" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_location/_id" type="string" calculate="../../parent" readonly="true()"/>
      <bind nodeset="/data/cases_group/outbreak_location/name" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_location/outbreak/name" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_location/outbreak/code" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_location/outbreak/disease" type="string"/>
      <bind nodeset="/data/cases_group/outbreak_location/outbreak/disease_name" type="string"/>
      <bind nodeset="/data/cases_group/name" type="string" required="true()"/>
      <bind nodeset="/data/cases_group/contact" type="string" calculate="coalesce( /data/init/select_person ,&quot;NEW&quot;)"/>
      <bind nodeset="/data/cases_group/metas/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/cases_group/metas/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/cases_group/metas/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id"/>
        <input ref="/data/inputs/user/facility_id"/>
        <input ref="/data/inputs/user/name"/>
      </group>
    </group>
    <group appearance="field-list" ref="/data/init">
      <select1 appearance="hidden" ref="/data/init/create_new_person">
        <label ref="jr:itext('/data/init/create_new_person:label')"/>
        <item>
          <label>New Person</label>
          <value>new_person</value>
        </item>
      </select1>
      <input appearance="db-object" ref="/data/init/select_person">
        <label ref="jr:itext('/data/init/select_person:label')"/>
      </input>
      <group appearance="field-list" ref="/data/contact">
        <input ref="/data/contact/title">
          <label ref="jr:itext('/data/contact/title:label')"/>
        </input>
        <group appearance="hidden" ref="/data/contact/meta"/>
        </group>
      </group>
    <group appearance="field-list" ref="/data/cases_group">
      <input ref="/data/cases_group/cha_outbreak_title">
        <label ref="jr:itext('/data/cases_group/cha_outbreak_title:label')"/>
      </input>
      <input ref="/data/cases_group/cha_outbreak_location_title">
        <label ref="jr:itext('/data/cases_group/cha_outbreak_location_title:label')"/>
      </input>
      <group appearance="field-list" ref="/data/cases_group/outbreak_location">
        <input appearance="hidden select-contact type-outbreak_location" ref="/data/cases_group/outbreak_location/_id">
          <label ref="jr:itext('/data/cases_group/outbreak_location/_id:label')"/>
        </input>
        <group appearance="field-list" ref="/data/cases_group/outbreak_location/outbreak"/>
      </group>
      <input ref="/data/cases_group/name">
        <label ref="jr:itext('/data/cases_group/name:label')"/>
      </input>
      <group appearance="hidden" ref="/data/cases_group/metas"/>
    </group>
  </h:body>
</h:html>
